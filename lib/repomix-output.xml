    OPUS: "libopus"
}

/** 音频编码器映射 */
export const AUDIO_ENCODER_MAP = Object.values(AUDIO_ENCODER).reduce((map, o) => {
    Object.entries(o).forEach(([type, codec]) => {
        !map[type] && (map[type] = []);
        map[type].push(codec)
    });
    return map;
}, {});

